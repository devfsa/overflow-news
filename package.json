{
  "name": "overflow-news",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "babel": "babel-node ./src",
    "babel-with-debug": "DEBUG=App* babel-node ./src",
    "build": "rm -rf ./build && npm run clean && npm run build:server && npm run copy:statics",
    "build:server": "babel src -d ./dist",
    "clean": "rm -rf ./dist",
    "copy:statics": "cp ./src/views ./dist/views -Rf && cp ./src/assets ./dist/assets -Rf",
    "lint": "eslint src/",
    "start:worker": "node ./dist/worker.js",
    "start:server": "node ./dist/server.js",
    "start:dev_worker": "nodemon ./src/worker.js --exec \"node -r dotenv/config -r babel-register\"",
    "start:dev_server": "nodemon ./src/server.js --exec \"node -r dotenv/config -r babel-register\"",
    "start": "npm run start:worker",
    "test:strict": "echo 'No tests found :('",
    "test": "npm run lint && npm run test:strict"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/di3goleite/overflow-news.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/di3goleite/overflow-news/issues"
  },
  "homepage": "https://overflow.news",
  "dependencies": {
    "bee-queue": "^1.2.2",
    "cron": "^1.3.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "feedparser": "^2.2.4",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "moment": "^2.21.0",
    "mongoose": "^5.0.9",
    "request": "^2.83.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2"
  }
}
